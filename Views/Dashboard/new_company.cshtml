@{
    ViewData["Title"] = "new_company";
    Layout = "~/Views/Shared/_DbLayout.cshtml";
}
<!-- Page Content-->
<div class="page-content">
    <div class="container-fluid">
        <!-- Page-Title -->
        <div class="row">
            <div class="col-sm-12">
                <div class="page-title-box">
                    <h4 class="page-title">All Companies</h4>
                </div>
                <!--end page-title-box-->
            </div>
            <!--end col-->
        </div>
        <!-- end page title end breadcrumb -->
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <div class="grid-x grid-padding-x block-border padding-2">
                            <div class="large-6 cell check-name-company-callback">

                                <div class="js-form-item form-item js-form-type-textfield form-item-title js-form-item-title">
                                    <label for="edit-title" class="form-required">Company Name</label>
                                    <input data-drupal-selector="edit-title" type="text" id="edit-title" name="title" value="" size="60" maxlength="128" class="form-text required edit-title" required="required" aria-required="true">

                                </div>
                            </div>
                            <div class="large-3 cell">

                                <div class="js-form-item form-item js-form-type-select form-item-field-company-type js-form-item-field-company-type form-disabled">
                                    <label for="edit-field-company-type">Company Type</label>
                                    <select disabled="disabled" data-drupal-selector="edit-field-company-type" id="edit-field-company-type" name="field_company_type" class="form-select"><option value="6" selected="selected" data-label="PTE. LTD.">PTE. LTD.</option><option value="2519" data-label="LTD.">LTD.</option></select>
                                </div>

                            </div><div class="large-3 cell position-relative">
                                <div class="invisible">checkname button</div><input class="check-name-btn expanded button js-form-submit form-submit edit-button" data-drupal-selector="edit-button" type="submit" id="edit-button" name="op" value="Check Name">
                            </div><input class="sgf-incorporate-company-next disabled hide button button--primary js-form-submit form-submit edit-next" data-drupal-selector="edit-next" type="submit" id="edit-next" name="op" value="Next">
                            <div id="devel-node-access-node-content-div">
                            </div>
                        </div>

                    </div>
                    <!--end card-body-->
                </div>
                <!--end card-->
            </div>
            <!--end col-->
        </div>
        <!--end row-->
    </div>
    <!-- container -->
    <footer class="footer text-center text-sm-left">&copy; 2019 Metrica <span class="text-muted d-none d-sm-inline-block float-right">Crafted with <i class="mdi mdi-heart text-danger"></i> by Mannatthemes</span></footer>
    <!--end footer-->
</div>
<!-- end page content -->
